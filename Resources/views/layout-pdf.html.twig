{% spaceless %}
<!DOCTYPE html>
    <head>
{# Meta #}
        {% block head %}
            <meta charset="utf-8" />
            <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
            <meta name="author" content="author" />
            <link rel="copyright" title="copyright" />
            <title>{{ title }}</title>
            <base />
        {% endblock %}
{# Css #}
{# Links MUST be absolute to be included in the PDF #}
        {% block css %}
            {# Uses c975L/IncludeLibraryBundle #}
            {{ inc_lib('bootstrap', 'css', '3.*') }}
            {{ inc_lib('fontawesome', 'css', '5.*') }}
{# Include your own styles #}
        {% endblock %}
    </head>
{# Body #}
    <body>
{# Header #}
        {{ include('@c975LGiftVoucher/fragments/header-pdf.html.twig') }}
{# Title #}
        <h1>
            {{ title }}
        </h1>
{# Content #}
        {% block giftVoucher_content %}
        {% endblock %}
{# Footer #}
        {{ include('@c975LGiftVoucher/fragments/footer-pdf.html.twig') }}
    </body>
</html>
{% endspaceless %}