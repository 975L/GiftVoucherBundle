{% spaceless %}
<!DOCTYPE html>
    <head>
        {% block head %}
            <meta charset="utf-8" />
            <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
            <meta name="viewport" content="width=device-width initial-scale=1.0 maximum-scale=1.0 user-scalable=no" />
            <meta name="format-detection" content="telephone=no" />
            <title>GiftVoucher ({{ title }})</title>
            <base />
        {% endblock %}
{# Css #}
            {% block css %}
                {# Uses c975L/IncludeLibraryBundle #}
                {{ inc_lib('bootstrap', 'css', '3.*') }}
                {{ inc_lib('fontawesome', 'css', '5.*') }}
            {% endblock %}
{# Javascript #}
            {% block javascript %}
                {# Uses c975L/IncludeLibraryBundle #}
                {{ inc_lib('jquery', 'js', '3.*') }}
                {{ inc_lib('bootstrap', 'js', '3.*') }}
            {% endblock %}
    </head>
{# Body #}
    <body>
{# Flashes #}
        {% for label, messages in app.flashes %}
            {% for message in messages %}
                <div class="flash alert alert-{{ label }} alert-dismissible text-center lead fade in" role="alert">
                    <button type="button" class="close" data-dismiss="alert" aria-label="close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    {{ message|raw|nl2br }}
                </div>
            {% endfor %}
        {% endfor %}

        <div class="container">
            {% block giftVoucher_content %}
            {% endblock %}
        </div>
    </body>
</html>
{% endspaceless %}