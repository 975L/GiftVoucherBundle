{
    "name": "c975l/giftvoucher-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to manage gift voucher, their payments and transactions",
    "keywords": ["symfony", "bundle", "gift", "voucher"],
    "homepage": "https://github.com/975L/GiftVoucherBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/Laurent3170",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": "^7",
        "c975l/config-bundle": "^2",
        "c975l/email-bundle": "^2",
        "c975l/includelibrary-bundle": "^1",
        "c975l/payment-bundle": "^2",
        "c975l/services-bundle": "^1",
        "c975l/toolbar-bundle": "^1",
        "cocur/slugify": "^2",
        "doctrine/doctrine-bundle": "^1",
        "doctrine/orm": "^2",
        "endroid/qr-code": "^3",
        "h4cc/wkhtmltopdf-amd64": "^0",
        "knplabs/knp-paginator-bundle": "^2",
        "knplabs/knp-snappy-bundle": "^1",
        "knplabs/knp-time-bundle": "^1",
        "symfony/form": "^4.2",
        "symfony/security": "^4.2",
        "twig/twig": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3@dev",
        "rector/rector": "^0.3",
        "symplify/easy-coding-standard": "^5.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c975L\\GiftVoucherBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/GiftVoucherBundle",
        "issues": "https://github.com/975L/GiftVoucherBundle/issues"
    }
}
